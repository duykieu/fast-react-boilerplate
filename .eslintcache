ts/AuthProvider\";\nimport { HashRouter } from \"react-router-dom\";\nimport Routers from \"./Routers\";\n\nconst App: React.FunctionComponent = ({ children }) => {\n    return (\n        <HashRouter>\n            <AuthProvider>\n                <Routers />\n            </AuthProvider>\n        </HashRouter>\n    );\n};\n\nexport default App;\n","/Users/jeffreyk/Sites/my-app/src/redux/reducer.ts",["208"],"import { Action, AnyAction, combineReducers } from \"redux\";\n\nimport auth from \"./auth\";\nimport posts from \"./post\";\nimport { ThunkAction, ThunkDispatch } from \"redux-thunk\";\n\nconst rootReducer = combineReducers({ auth, posts });\n\nexport type RootStateType = ReturnType<typeof rootReducer>;\n\nexport type AppThunk<ReturnType = void> = ThunkAction<\n    ReturnType,\n    RootStateType,\n    unknown,\n    Action<string>\n>;\n\nexport type ReduxThunkDispatch = ThunkDispatch<RootStateType, void, Action>;\n\nexport type DefaultAction<T> = {\n    type: string;\n    payload?: T;\n};\n\nexport default rootReducer;\n","/Users/jeffreyk/Sites/my-app/src/redux/auth.ts",["209","210","211","212"],"import User, { IUser, IUserLoginSuccessPayload } from \"../models/user\";\nimport CoreAction, { ThunkDispatch } from \"./core_action\";\nimport { AppThunk, RootStateType } from \"./reducer\";\nimport { Action, ActionCreator, Reducer } from \"redux\";\nimport { ThunkAction } from \"redux-thunk\";\n\nexport interface IAuthState {\n    token: string | null;\n    currentUser: IUser | null;\n    loading: boolean;\n    error: any;\n    params?: {};\n}\n\nconst initialState: IAuthState = {\n    token: null,\n    currentUser: null,\n    loading: false,\n    error: null,\n    params: {}\n};\n\nconst AUTH_REQUESTED = \"AUTH_REQUESTED\";\nconst AUTH_REQUEST_SUCCESS = \"AUTH_REQUEST_SUCCESS\";\nconst AUTH_REQUEST_FAILED = \"AUTH_REQUEST_FAILED\";\nconst AUTH_LOGOUT_USER = \"AUTH_LOGOUT_USER\";\nconst AUTH_SET_ERROR = \"AUTH_SET_ERROR\";\n\nexport default (\n    state = initialState,\n    action: { type: string; payload: any }\n): IAuthState => {\n    switch (action.type) {\n        case AUTH_REQUESTED:\n            return {\n                ...state,\n                loading: true\n            };\n        case AUTH_REQUEST_SUCCESS:\n            return {\n                ...state,\n                loading: false,\n                ...action.payload\n            };\n        case AUTH_REQUEST_FAILED:\n            return {\n                ...state,\n                loading: false,\n                error: action.payload\n            };\n        case AUTH_SET_ERROR:\n            return {\n                ...state,\n                error: action.payload\n            };\n        case AUTH_LOGOUT_USER:\n            return initialState;\n        default:\n            return state;\n    }\n};\n\nclass AuthActionCreators extends CoreAction<typeof User> {\n    constructor() {\n        super({\n            model: User,\n            requestedAction: AUTH_REQUESTED,\n            successAction: AUTH_REQUEST_SUCCESS,\n            failedAction: AUTH_REQUEST_FAILED,\n            errorAction: AUTH_SET_ERROR\n        });\n    }\n\n    login(payload: {\n        username: string;\n        password: string;\n    }): ThunkAction<void, RootStateType, unknown, Action<string>> {\n        return (dispatch: ThunkDispatch) => {\n            return User.login<IUserLoginSuccessPayload>(payload)\n                .then(data => {\n                    dispatch(\n                        this.requestSuccess<IUserLoginSuccessPayload>(data)\n                    );\n                })\n                .catch(e => {\n                    dispatch(this.setError(e));\n                });\n        };\n    }\n}\n\nexport const AuthAction = new AuthActionCreators();\n","/Users/jeffreyk/Sites/my-app/src/redux/core_action.ts",["213","214","215","216"],"/Users/jeffreyk/Sites/my-app/src/models/user.ts",[],"/Users/jeffreyk/Sites/my-app/src/models/core_model.ts",[],"/Users/jeffreyk/Sites/my-app/src/utils.ts",[],"/Users/jeffreyk/Sites/my-app/src/config.ts",[],"/Users/jeffreyk/Sites/my-app/src/components/AuthProvider.tsx",["217"],"import React from \"react\";\nimport { connect } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport { RootStateType } from \"../redux/reducer\";\nimport { TOKEN_KEY } from \"../config\";\n\nconst AuthProvider: React.FC<{\n    token: string | null;\n    children: React.PropsWithChildren<any>;\n}> = ({ children, token }) => {\n    const history = useHistory();\n\n    // We write down token to localStorage for quick access from model\n    React.useEffect(() => {\n        if (token) {\n            localStorage.setItem(TOKEN_KEY, token);\n        } else {\n            const { location } = history;\n            localStorage.removeItem(TOKEN_KEY);\n            if (location.pathname !== \"/login\") {\n                history.push(\"/login\");\n            }\n        }\n    }, [token]);\n\n    return children;\n};\n\nconst mapStateToProps = ({ auth }: RootStateType) => ({\n    token: auth.token\n});\n\nexport default connect(mapStateToProps)(AuthProvider);\n","/Users/jeffreyk/Sites/my-app/src/Routers.tsx",[],"/Users/jeffreyk/Sites/my-app/src/pages/LoginPage.tsx",["218","219"],"/Users/jeffreyk/Sites/my-app/src/redux/post.ts",["220"],"import Post, { IPost } from \"../models/post\";\nimport { DefaultAction } from \"./reducer\";\nimport CoreAction from \"./core_action\";\n\nexport interface IPostsState {\n    posts: IPost[];\n    loading: boolean;\n    error: any;\n}\n\nconst initialState: IPostsState = {\n    posts: [],\n    loading: false,\n    error: null\n};\n\nconst POSTS_REQUESTED = \"POST_REQUESTED\";\nconst POSTS_REQUEST_SUCCESS = \"POST_REQUEST_SUCCESS\";\nconst POSTS_REQUEST_FAILED = \"POST_REQUEST_FAILED\";\nconst POSTS_SET_ERROR = \"POST_SET_ERROR\";\n\nexport default (state = initialState, action: DefaultAction<any>) => {\n    switch (action.type) {\n        case POSTS_REQUESTED:\n            return {\n                ...state,\n                loading: true\n            };\n        case POSTS_REQUEST_SUCCESS:\n            return {\n                ...state,\n                ...action.payload,\n                loading: false\n            };\n        case POSTS_REQUEST_FAILED:\n            return {\n                ...state,\n                error: action.payload,\n                loading: false\n            };\n        case POSTS_SET_ERROR:\n            return {\n                ...state,\n                error: action.payload\n            };\n        default:\n            return state;\n    }\n};\n\nclass PostActionCreators extends CoreAction<typeof Post> {\n    constructor() {\n        super({\n            model: Post,\n            requestedAction: POSTS_REQUESTED,\n            failedAction: POSTS_REQUEST_FAILED,\n            successAction: POSTS_REQUEST_SUCCESS,\n            errorAction: POSTS_SET_ERROR\n        });\n    }\n}\n\nexport const PostAction = new PostActionCreators();\n","/Users/jeffreyk/Sites/my-app/src/models/post.ts",[],"/Users/jeffreyk/Sites/my-app/src/pages/HomePage.tsx",["221"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/index.tsx",[],["222","223"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/reportWebVitals.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/store.ts",["224","225"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/App.tsx",["226"],"import React from \"react\";\nimport logo from \"./logo.svg\";\nimport AuthProvider from \"./components/AuthProvider\";\nimport { HashRouter } from \"react-router-dom\";\nimport Routers from \"./Routers\";\n\nimport \"antd/dist/antd.css\";\n\nconst App: React.FunctionComponent = ({ children }) => {\n    return (\n        <HashRouter>\n            <AuthProvider>\n                <Routers />\n            </AuthProvider>\n        </HashRouter>\n    );\n};\n\nexport default App;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/reducer.ts",["227"],"import { Action, AnyAction, combineReducers } from \"redux\";\n\nimport auth from \"./auth\";\nimport global from \"./global\";\nimport { ThunkAction, ThunkDispatch } from \"redux-thunk\";\n\nconst rootReducer = combineReducers({ global, auth });\n\nexport type RootStateType = ReturnType<typeof rootReducer>;\n\nexport type AppThunk<ReturnType = void> = ThunkAction<\n    ReturnType,\n    RootStateType,\n    unknown,\n    Action<string>\n>;\n\nexport type ReduxThunkDispatch = ThunkDispatch<RootStateType, void, Action>;\n\nexport type DefaultAction<T> = {\n    type: string;\n    payload?: T;\n};\n\nexport default rootReducer;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/Routers.tsx",["228"],"import { Switch, Route } from \"react-router-dom\";\n\nimport LoginPage from \"./pages/LoginPage\";\nimport HomePage from \"./pages/HomePage\";\nimport Layout from \"./components/Layout\";\n\nconst Routers: React.FunctionComponent = () => {\n    return (\n        <Switch>\n            <Route path=\"/login\" component={LoginPage} />\n            <Layout />\n        </Switch>\n    );\n};\n\nexport default Routers;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/components/AuthProvider.tsx",["229"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/auth.ts",["230","231","232","233"],"import User, { IUser, IUserLoginSuccessPayload } from \"../models/user\";\nimport CoreAction, { ThunkDispatch } from \"./core_action\";\nimport { AppThunk, RootStateType } from \"./reducer\";\nimport { Action, ActionCreator, Reducer } from \"redux\";\nimport { ThunkAction } from \"redux-thunk\";\n\nexport interface IAuthState {\n    token: string | null;\n    currentUser: IUser | null;\n    loading: boolean;\n    error: any;\n    params?: {};\n}\n\nconst initialState: IAuthState = {\n    token: null,\n    currentUser: null,\n    loading: false,\n    error: null,\n    params: {}\n};\n\nconst AUTH_SET_LOADING = \"AUTH_SET_LOADING\";\nexport const AUTH_SET_USER = \"AUTH_SET_USER\";\nexport const AUTH_LOGOUT_USER = \"AUTH_LOGOUT_USER\";\nconst AUTH_SET_ERROR = \"AUTH_SET_ERROR\";\n\nexport default (\n    state = initialState,\n    action: { type: string; payload: any }\n): IAuthState => {\n    switch (action.type) {\n        case AUTH_SET_LOADING:\n            return {\n                ...state,\n                loading: true\n            };\n\n        case AUTH_SET_ERROR:\n            return {\n                ...state,\n                error: action.payload\n            };\n        case AUTH_SET_USER:\n            return {\n                ...state,\n                ...action.payload\n            };\n        case AUTH_LOGOUT_USER:\n            return initialState;\n        default:\n            return state;\n    }\n};\n\nclass AuthActionCreators extends CoreAction<typeof User, IUser> {\n    constructor() {\n        super({\n            model: User,\n            loadingAction: AUTH_SET_LOADING,\n            errorAction: AUTH_SET_ERROR\n        });\n    }\n\n    setUser(payload) {\n        return {\n            type: AUTH_SET_USER,\n            payload\n        };\n    }\n\n    login(payload: {\n        username: string;\n        password: string;\n    }): ThunkAction<void, RootStateType, unknown, Action<string>> {\n        return this.run((dispatch: ThunkDispatch) => {\n            return User.login<IUserLoginSuccessPayload>(payload).then(data => {\n                dispatch(this.setUser(data));\n            });\n        });\n    }\n}\n\nexport const AuthAction = new AuthActionCreators();\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/config.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/pages/HomePage.tsx",["234","235"],"import React from \"react\";\nimport styled from \"styled-components\";\nimport Layout from \"../components/Layout\";\n\ninterface IHomePageProps {}\n\nconst HomePage: React.FunctionComponent<IHomePageProps> = () => {\n    return <h1>Hello from home page</h1>;\n};\n\nexport default HomePage;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/pages/LoginPage.tsx",["236","237"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/core_action.ts",["238"],"// @ts-ignore\nimport Model from \"../models/core\";\nimport {\n    AppThunk,\n    DefaultAction,\n    RootStateType,\n    ReduxThunkDispatch\n} from \"./reducer\";\nimport { ThunkAction, ThunkDispatch as LibThunkDispatch } from \"redux-thunk\";\n\nexport type ThunkDispatch = LibThunkDispatch<\n    RootStateType,\n    void,\n    Action<string>\n>;\n\ninterface Action<Payload> {\n    type: string;\n    payload?: Payload;\n    error?: boolean;\n}\n\ninterface IActionInitialValue<M> {\n    model: M;\n    fetchCallback?: any;\n    errorAction: string;\n    loadingAction: string;\n}\n\nexport interface IFetchResultWithPaginate<T> {\n    results: T[];\n    count: number;\n}\n\nexport default class CoreAction<M extends typeof Model, T> {\n    errorAction: string;\n\n    loadingAction: string;\n\n    fetchCallback: any;\n\n    model: M;\n\n    constructor({\n        model,\n        fetchCallback,\n        errorAction,\n        loadingAction\n    }: IActionInitialValue<M>) {\n        this.errorAction = errorAction;\n        this.loadingAction = loadingAction;\n        this.model = model;\n        this.fetchCallback = fetchCallback;\n    }\n\n    run<T>(fn): (dispatch, getState?) => Promise<T> {\n        return (dispatch): Promise<T> => {\n            dispatch(this.setLoading(true));\n\n            return fn(dispatch)\n                .catch(e => {\n                    dispatch(this.setError(e));\n                })\n                .finally(() => {\n                    dispatch(this.setLoading(false));\n                });\n        };\n    }\n\n    setLoading(payload: boolean) {\n        return {\n            type: this.loadingAction,\n            payload\n        };\n    }\n\n    clearError(): DefaultAction<any> {\n        return {\n            type: this.errorAction,\n            payload: null\n        };\n    }\n\n    setError<T>(payload: T): DefaultAction<T> {\n        return {\n            type: this.errorAction,\n            payload\n        };\n    }\n\n    fetch(params?: any) {\n        return this.run(dispatch => {\n            return this.model.find(params).then(data => {\n                if (this.fetchCallback) this.fetchCallback(dispatch, data);\n            });\n        });\n    }\n\n    create(payload): AppThunk<Promise<T | void>> {\n        return this.run((dispatch: ReduxThunkDispatch) => {\n            return this.model.create<T, any>(payload).then(data => {\n                // @ts-ignore\n                dispatch(this.fetch());\n                return data;\n            });\n        });\n    }\n\n    update(id: string, payload: T): AppThunk<Promise<T | void>> {\n        return this.run((dispatch: ReduxThunkDispatch) => {\n            return this.model.update<T, T>(id, payload).then(data => {\n                // @ts-ignore\n                dispatch(this.fetch());\n                return data;\n            });\n        });\n    }\n\n    destroy(id: string | undefined): AppThunk {\n        return this.run((dispatch: ReduxThunkDispatch) => {\n            return this.model.destroy(id).then(() => {\n                // @ts-ignore\n                dispatch(this.fetch());\n            });\n        });\n    }\n}\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/models/user.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/models/post.ts",[],["239","240"],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/components/Layout.tsx",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/models/core_model.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/components/TopBar.tsx",["241","242"],"import React from \"react\";\nimport styled from \"styled-components\";\nimport { Menu } from \"react-feather\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { Link } from \"react-router-dom\";\n\nimport { Colors } from \"../Colors\";\nimport { useGlobalHooks } from
   slug = \"@\" + slug + \"@\";\n    slug = slug.replace(/\\@\\-|\\-\\@|\\@/gi, \"\");\n    //In slug ra textbox có id “slug”\n    return slug;\n};\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/Colors.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/data/menu.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/global.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/redux/global_hooks.ts",[],"/Users/jeffreyk/Desktop/fast-react-boilerplate/src/pages/ProductsPage.tsx",["280"],"import React from \"react\";\nimport styled from \"styled-components\";\nimport Breadcrumbs from \"../components/Breadcrumbs\";\n\ninterface IProductsProps {}\n\nconst ProductsPage: React.FunctionComponent<IProductsProps> = () => {\n    return <Breadcrumbs items={[]} />;\n};\n\nexport default ProductsPage;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/pages/SettingsPage.tsx",["281"],"import React from \"react\";\nimport styled from \"styled-components\";\n\ninterface ISettingsPageProps {}\n\nconst SettingsPage: React.FunctionComponent<ISettingsPageProps> = () => {\n    return <h1>Hello from Setting page</h1>;\n};\n\nexport default SettingsPage;\n","/Users/jeffreyk/Desktop/fast-react-boilerplate/src/components/Breadcrumbs.tsx",["282"],"import React from \"react\";\nimport { Breadcrumb } from \"antd\";\nimport { ChevronRight } from \"react-feather\";\nimport styled from \"styled-components\";\n\ninterface IBreadcrumbItem {\n    label: string;\n    path?: string;\n}\n\ninterface IBreadcrumbsProps {\n    items: IBreadcrumbItem[];\n}\n\nconst Breadcrumbs: React.FunctionComponent<IBreadcrumbsProps> = ({ items }) => {\n    return (\n        <Breadcrumb\n            separator={\n                <ChevronRight size={14} style={{ marginBottom: \"-3px\" }} />\n            }\n        >\n            <Breadcrumb.Item href=\"/\">Trang chủ</Breadcrumb.Item>\n            {items.map((item, index) => (\n                <Breadcrumb.Item key={index} href={item.path}>\